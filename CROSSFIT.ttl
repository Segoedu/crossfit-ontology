###  http://www.semanticweb.org/user/ontologies/2023/10/crossfit#Fran
:Fran rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :WOD
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasEstiloWOD ;
                                                   owl:allValuesFrom :ForRounds
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasExercise ;
                                                   owl:allValuesFrom [ owl:intersectionOf ( :Pullup
                                                                                            :Thruster
                                                                                          ) ;
                                                                       rdf:type owl:Class
                                                                     ]
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :WOD ;
      owl:disjointWith :Murph ;
      rdfs:comment """The workout:

Three rounds, 21-15- and 9 reps, for time of:

    43/30 kg (95 lb) thruster
    Pull-ups""" ;
      rdfs:label "Fran" ;
      rdfs:seeAlso "https://www.youtube.com/watch?v=KYaqy4zcAJg&t=3s&ab_channel=CrossFit" .


