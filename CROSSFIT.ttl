###  http://www.semanticweb.org/user/ontologies/2023/10/crossfit#Murph
:Murph rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :WOD
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasEstiloWOD ;
                                                    owl:allValuesFrom :Fortime
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasExercise ;
                                                    owl:allValuesFrom [ owl:intersectionOf ( :AirSquat
